{
  "name": "visortest_app",
  "version": "0.1.0",
  "private": true,
  "description": "Client application for the Visor.ai test chatbot",
  "scripts": {
    "prepare": "cd ../ && husky install ./client/.husky",
    "dev": "next dev",
    "next:build": "next build",
    "next:start": "next start",
    "build": "npm-run-all next:build lint prettier",
    "start": "npm-run-all build next:start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --config .prettierrc --check .",
    "prettier:fix": "prettier --config .prettierrc --write ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:fix && npm run lint:fix"
    }
  },
  "dependencies": {
    "@types/node": "18.16.2",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "autoprefixer": "10.4.14",
    "next": "13.3.1",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-next": "13.3.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  }
}
